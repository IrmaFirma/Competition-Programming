#include <iostream>
#include <vector>
#include<map>
#include <algorithm>
#include<cmath>
using namespace std;
 
 
int main()
{
    long long n, suma=0, broj;
    cin>>n;
    for(int i=0;i<n-1;i++){
        cin>>broj;
        suma+=broj;
    }
    
    cout<< (n*(n+1))/2 - suma <<endl;
    
    
 	return 0;
}
